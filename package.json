{
  "name": "vue-chess",
  "description": "A Vue.js experiment",
  "author": "psimyn",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --content-base ./ --inline --hot",
    "build": "NODE_ENV=production webpack --progress --hide-modules",
    "test": "mocha-webpack --webpack-config webpack.test.js \"src/**/*test.js\" && ava",
    "test:watch": "ava --watch"
  },
  "ava": {
    "files": [
      "src/**/*-test.js"
    ],
    "require": [
      "babel-polyfill",
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "vue": "^2.0.0-beta.5",
    "vuex": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.0.2",
    "chess": "^0.2.7",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-plugin-html": "^2.0.3",
    "file-loader": "^0.11.2",
    "firebase": "^4.1.2",
    "inject-loader": "^3.0.0-beta1",
    "json-loader": "^0.5.4",
    "localforage": "^1.4.2",
    "mocha": "^3.0.2",
    "mocha-webpack": "^0.7.0",
    "standard": "^10.0.2",
    "vue-loader": "^12.2.1",
    "vue-template-compiler": "^2.3.4",
    "vuex-devtools": "^0.1.0",
    "vuexfire": "^2.1.3",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-node-externals": "^1.4.3"
  }
}
