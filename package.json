{
  "name": "airchess",
  "description": "Chess",
  "author": "psimyn",
  "private": true,
  "scripts": {
    "prebuild": "mkdir -p dist && xargs -a pages-files cp -p -t dist",
    "dev": "webpack-dev-server --content-base ./dist --inline --hot --host 0.0.0.0 --public 192.168.0.10:8081",
    "start": "npm run dev",
    "build": "NODE_ENV=production webpack --progress --hide-modules",
    "build:all": "npm run prebuild && npm run build && workbox generateSW",
    "deploy": "vim changelog && git commit changelog -m changelog && npm run build:all && gh-pages -d dist",
    "stats": "webpack --profile --json > stats.json",
    "test": "jest"
  },
  "dependencies": {
    "element-ui": "^2.2.2",
    "raw-loader": "^0.5.1",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@firebase/app-types": "^0.3.2",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-component": "^1.1.1",
    "chai": "^4.0.2",
    "chess": "^0.3.3",
    "cross-env": "^5.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-plugin-html": "^4.0.6",
    "file-loader": "^2.0.0",
    "firebase": "^5.5.4",
    "firebase-mock": "^2.2.10",
    "firebaseui": "^3.4.1",
    "gh-pages": "^2.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "json-loader": "^0.5.4",
    "localforage": "^1.4.2",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.16",
    "vuexfire": "^2.1.3",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-node-externals": "^1.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "transform": {
      ".*\\.(js)$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  }
}